{
  "name": "eslint-plugin-react-native",
  "version": "3.7.0",
  "author": "Tom Hastjarjanto <tom@intellicode.nl>",
  "description": "React Native specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
